<div class="panel panel-default ">
    <div class="panel-heading main-bgcolors">
    <h3 class="panel-title">Website overview</h3>
    </div>
    <div class="panel-body">
    <div class="col-md-3">
        <div class="well text-center">
        <h2><span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
            {{= data.modules}}
        </h2>
        <h4>Modules</h4>
        </div>
    </div>
    <div class="col-md-3">
        <div class="well text-center">
        <h2><span class="glyphicon glyphicon-list-alt" aria-hidden="true"></span>
            {{= data.games}}
        </h2>
        <h4>Games</h4>
        </div>
    </div>
    <div class="col-md-3">
        <div class="well text-center">
        <h2><span class="glyphicon glyphicon-clock" aria-hidden="true"></span>
            {{= data.hours}}
        </h2>
        <h4>Hours Played</h4>
        </div>
    </div>
    <div class="col-md-3">
        <div class="well text-center">
        <h2><span class="glyphicon glyphicon-stats" aria-hidden="true"></span>
            {{= data.players}}
        </h2>
        <h4>Players</h4>
        </div>
    </div>
    </div>
</div>
